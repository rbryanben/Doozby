<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wapazock.doozby">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Doozby"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".SplashScreen.Splash"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WelcomePage.WelcomeActivity"/>
        <activity android:name=".CreateAccountPage.CreateUsernameActivity"/>
        <activity android:name=".CreateAccountPage.CreatePasswordActivity" />
        <activity android:name=".SignInPage.SignInActivity"/>
        <activity android:name=".MainActivity"/>
    </application>

</manifest>